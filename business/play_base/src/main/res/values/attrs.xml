<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundCornerImageView">
        <attr name="round_corner_radius" format="dimension" />
        <attr name="round_corner_radius_left_top" format="dimension" />
        <attr name="round_corner_radius_left_bottom" format="dimension" />
        <attr name="round_corner_radius_right_top" format="dimension" />
        <attr name="round_corner_radius_right_bottom" format="dimension" />
        <attr name="round_corner_border_width" format="dimension"/>
        <attr name="round_corner_border_color" format="color|reference"/>
    </declare-styleable>


    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer" />
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="WebView">
        <!-- require String render to html show in webview-->
        <attr name="loadJsBridgeHtml" format="string" />
        <attr name="loadJsDefaultHandle" format="reference" />
        <attr name="setWebChromeClient" format="reference" />
        <attr name="setWebViewClient" format="reference" />
        <attr name="registerHandler" format="reference" />
        <attr name="send" format="string" />
        <attr name="sendMessage" format="string" />
        <attr name="handlerName" format="string" />
        <attr name="handlerData" format="string" />
        <attr name="callBackFunction" format="reference" />
    </declare-styleable>

</resources>